//Headings
h1,h2,h3,h4,h5,h6
 color color_ui1_font_pattern1
 font-family font_pattern1
 font-weight normal
 line-height 1.2em
h1 
 font-size 1.6em
 text-transform uppercase
h2
 font-size 1.4em  
h3
 font-size 1.15em
h4
 font-size 1em
h5
 font-size 1.1em
h6
 font-size 0.9em
//BASE
a 
 color color_ui1_links1
 text-decoration none  
 &:hover
  text-decoration underline  
small 
 font-size 0.8em 
abbr 
 border-bottom 1px dotted color_ui1_border_pattern1
code
 background #efefef 
 border-radius(4px)
 border 1px solid color_ui1_border_pattern1
 font 0.9em/1.15em font_pattern1
 padding 2px 4px    
address
 font-style normal
blockquote
 border-left 5px solid color_ui1_border_pattern1
 padding 8px 10px
 p:last-child
  margin 0  
//List
ul,ol
 padding 0 0 0 30px
ul ul
 margin 10px 0    
dt 
 font-weight bold
//Tables
table
 border-collapse collapse
 border-spacing 0
 table-layout fixed
 width 100%
 word-wrap break-word
thead
  tr
   background bg_patter5
  th
   color color_ui1_font_pattern2
   font 1em/1.2em font_pattern1
   padding 19px 12px
   text-align left
   transition(0.2s)
tbody td,tfoot td
 padding 10px 12px
 transition(0.2s)
tfoot td
 font bold 13px/18px font_pattern1        
//FORM
select,textarea,input[type=text],input[type=password],input[type=datetime],input[type=date],input[type=month],input[type=time],input[type=week],input[type=number],input[type=email],input[type=url],input[type=tel],input[type=color]
 background none
 border 0
 color color_ui1_font_pattern1
 display block
 font-family font_pattern1
 font-size 1.1em
 height 38px
 outline none
 padding 5px
 width 100%
::-webkit-input-placeholder
 color color_ui1_font_pattern1
::-moz-placeholder
 color color_ui1_font_pattern1
:-moz-placeholder
 color color_ui1_font_pattern1
:-ms-input-placeholder
 color color_ui1_font_pattern1
 label
  cursor pointer 
  margin 0     
input[type=checkbox],input[type=radio]
 cursor pointer
 display block
 float left
 margin 0 3px 0 0
 height 16px
 width 16px
fieldset
 border 1px solid color_ui1_border_pattern1
 border-radius(5px)
 padding 10px
legend
 color #58585a
label
 color color_ui1_font_pattern1
 display block
 margin 5px 0 0 0
select
 height 2.6em
 line-height: 1em
 width 100%
select[multiple]
 height auto
textarea
 font-family font_pattern1
 height auto
input[type=submit],button
 border none
 cursor pointer
 display block
 text-align left 
button,input[type=submit]
 background bg_ui1_patter1
 color color_ui1_font_pattern1
 cursor pointer
 display block
 font 1.1em/2.3em font_pattern2
 text-align center
 text-transform uppercase
 width 100%
 transition(0.3s) 